{
  "manifestVersion": 1,
  "id": "upload-sp-files-app",
  "name": "SharePoint Files Uploader",
  "version": "0.2.2",
  "publisher": "projectumapsvsts",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.build"
  ],
  "description": "Defines a release task to upload files to SharePoint document library.",
  "categories": [
    "Build and release"
  ],
  "icons": {
    "default": "UploadFilesToSPDocLib/icon.png"
  },
  "files": [
    {
      "path": "UploadFilesToSPDocLib"
    }
  ],
  "contributions": [
    {
      "id": "upload-sp-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "UploadFilesToSPDocLib"
      }
    }
  ]
}